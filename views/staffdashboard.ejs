                <div class="dashboard_inner">
                    <div class="db_dataCard">
                        <div class="dataCard d_routes">
                            <div class="db_cardIcon">
                                <i class="bi bi-clipboard2-check-fill"></i>
                            </div>
                            <div class="db_dataText">
                                <h4>40</h4>
                                <h3>Total Routes</h3>
                               <p class="lastUp"><i class="bi bi-calendar-check"></i> Last Updated: 12/5/2022 22:0</p>
                            </div>
                        </div>
                        <div class="dataCard d_stops">
                            <div class="db_cardIcon">
                                <i class="bi bi-truck"></i>
                            </div>
                            <div class="db_dataText">
                                <h4>42/1710</h4>
                                <h3>Delivery Stops</h3>
                                <p class="lastUp"><i class="bi bi-calendar-check"></i> Last Updated: 12/5/2022 22:0</p>
                                
                            </div>
                        </div>
                        <div class="dataCard d_pickup">
                            <div class="db_cardIcon">
                                <i class="bi bi-truck"></i>
                            </div>
